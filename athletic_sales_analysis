        "# Show the total sales for the products sold for each retailer, region, state, and city.\n",
        "# Rename the \"total_sales\" column to \"Total Sales\"\n",
        "group_total_sales_product = sales_2020_2021_df.groupby(['retailer', 'region', 'state', 'city'])['total_sales'].sum().reset_index()\n",
        "\n",
        "# Rename the \"total_sales\" column to \"Total Sales\"\n",
        "group_total_sales_product = group_total_sales_product.rename(columns={'total_sales': 'Total Sales'})\n",
        "\n",
        "# Show the top 5 results\n",
        "group_top_sales_product = group_total_sales_product.nlargest(5, 'Total Sales')\n",
        "group_top_sales_product"
