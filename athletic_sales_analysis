      "source": [
        "### 5. Determine the Day with the Most Women's Athletic Footwear Sales"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 30,
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "MPy2ONMQVKXH",
        "outputId": "9f1c498d-5668-4180-b638-9dd5b4a91ef2"
      },
      "outputs": [
        {
          "output_type": "execute_result",
          "data": {
            "text/plain": [
              "invoice_date    2021-07-16 00:00:00\n",
              "Total Sales                   22857\n",
              "Name: 554, dtype: object"
            ]
          },
          "metadata": {},
          "execution_count": 30
        }
      ],
      "source": [
        "# Create a pivot table with the 'invoice_date' column is the index, and the \"total_sales\" as the values.\n",
        "\n",
        "pivot_max_sales_day = pd.pivot_table(sales_2020_2021_df,\n",
        "                                     values='units_sold',\n",
        "                                     index='invoice_date',\n",
        "                                     aggfunc='sum').reset_index()\n",
        "\n",
        "# Optional: Rename the \"units_sold\" column to \"Total Sales\"\n",
        "pivot_max_sales_day = pivot_max_sales_day.rename(columns={'units_sold': 'Total Sales'})\n",
        "\n",
        "# Find the day with the maximum total sales\n",
        "day_with_max_sales = pivot_max_sales_day.loc[pivot_max_sales_day['Total Sales'].idxmax()]\n",
        "day_with_max_sales"
      ]
    },
