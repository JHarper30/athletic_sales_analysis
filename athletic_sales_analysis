        "# Show the total sales for the products sold for each retailer, region, state, and city.\n",
        "\n",
        "pivot_total_sales_product = pd.pivot_table(sales_2020_2021_df,\n",
        "                             values='total_sales',\n",
        "                             index=['retailer', 'region', 'state', 'city'],\n",
        "                             aggfunc='sum').reset_index()\n",
        "\n",
        "# Optional: Rename the \"total_sales\" column to \"Total Sales\"\n",
        "pivot_total_sales_product = pivot_total_sales_product.rename(columns={'total_sales': 'Total Sales'})\n",
        "\n",
        "# Show the top 5 results\n",
        "pivot_top_sales_product = pivot_total_sales_product.nlargest(5, 'Total Sales')\n",
        "\n",
        "pivot_top_sales_product"
